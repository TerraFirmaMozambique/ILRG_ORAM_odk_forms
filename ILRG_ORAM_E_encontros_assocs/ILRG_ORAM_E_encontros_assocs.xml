            <value form="image">jr://images/oram_USAID.jpg</value>
          </text>
        </translation>
      </itext>
      <instance>
        <ILRG_ORAM_E_encontros_assocs id="ILRG_ORAM_E" version="201902281">
          <start/>
          <deviceid/>
          <my_form_name/>
          <Eg1>
            <logo/>
            <cavateco/>
          </Eg1>
          <intronote/>
