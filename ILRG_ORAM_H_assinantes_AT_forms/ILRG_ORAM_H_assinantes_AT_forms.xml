        <translation default="true()" lang="default">
          <text id="/ILRG_ORAM_H_assinantes_AT_forms/Hg1/logo:label">
            <value form="image">jr://images/oram_USAID.jpg</value>
          </text>
        </translation>
      </itext>
      <instance>
        <ILRG_ORAM_H_assinantes_AT_forms id="ILRG_ORAM_H" version="201904101">
          <start/>
          <deviceid/>
          <my_form_name/>
          <Hg1>
            <logo/>
            <cavateco/>
          </Hg1>
          <intronote/>
